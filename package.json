{
  "name": "react-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "chokidar-cli": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "^3.8.0"
  },
  "scripts": {
    "prestart": "run-s build:css",
    "start": "run-p watch:css start:react",
    "start:react": "react-scripts start",
    "prebuild": "run-s build:css",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/tailwind.output.css",
    "watch:css": "chokidar 'src/**/*.css' 'src/**/*.scss' 'tailwind.js' --ignore src/assets/tailwind.output.css -c 'npm run build:css'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "tailwindcss": "^1.7.6"
  }
}